{
  "name": "P52",
  "title": "Platform P52 (ESP32)",
  "description": "Espressif Systems is a privately held fabless semiconductor company. They provide wireless communications and Wi-Fi chips which are widely used in mobile devices and the Internet of Things applications.",
  "homepage": "https://espressif.com/",
  "license": "Apache-2.0",
  "version": "3.1.1",
  "engines": {
    "platformio": "^5"
  },
  "keywords": [
    "OS-Q",
    "dev-platform",
    "Wi-Fi",
    "Bluetooth",
    "Xtensa",
    "LX6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P52.git"
  },
  "frameworks": {
    "arduino": {
      "package": "A52A",
      "script": "builder/extend/A52A.py"
    },
    "espidf": {
      "package": "E52A",
      "script": "builder/extend/E52A.py"
    },
    "simba": {
      "package": "simba",
      "script": "builder/extend/simba.py"
    },
    "pumbaa": {
      "package": "E52B",
      "script": "builder/extend/E52B.py"
    }
  },
  "packages": {
    "toolchain-xtensa32": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~2.50200.0",
      "optionalVersions": ["~2.80400.0"]
    },
    "toolchain-xtensa32s2": {
      "type": "toolchain",
      "optional": true,
      "owner": "platformio",
      "version": "~1.80400.0"
    },
    "toolchain-esp32ulp": {
      "type": "toolchain",
      "optional": true,
      "owner": "platformio",
      "version": "~1.22851.0"
    },
    "toolchain-esp32s2ulp": {
      "type": "toolchain",
      "optional": true,
      "owner": "platformio",
      "version": "~1.22851.0"
    },
    "A52A": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/A52A.git"
    },
    "E52A": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/E52A.git"
    },
    "E52B": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/E52B.git"
    },
    "simba": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/simba.git"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "owner": "platformio",
      "version": "~1.30000.0"
    },
    "tool-mbctool": {
      "optional": true,
      "owner": "meteca",
      "version": ">=2.0.0"
    },
    "tool-openocd-esp32": {
      "type": "debugger",
      "optional": true,
      "owner": "platformio",
      "version": "~2.1000.0"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~2.230.0"
    },
    "tool-cmake": {
      "optional": true,
      "owner": "platformio",
      "version": "~3.16.0"
    },
    "tool-ninja": {
      "optional": true,
      "owner": "platformio",
      "version": "^1.7.0"
    },
    "tool-mconf": {
      "optional": true,
      "owner": "platformio",
      "version": "~1.4060000.0"
    },
    "tool-idf": {
      "optional": true,
      "owner": "platformio",
      "version": "~1.0.1"
    }
  }
}
